    function doGoBack(i) {
	if (isPreview)
	    return false;

	history_length = history.length;
	if (Browser.IE) {
		history_length = history_length + 1;
	}
	if(history_length > i*(-1)) {
	    history.go(i);
	} else {
            window.location = 'about:blank';
	}

	return false;
    }

